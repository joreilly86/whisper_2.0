# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Configuration (optional, will fallback to OpenAI)
GEMINI_API_KEY=your_gemini_api_key_here

# Groq API Configuration (optional, will be used as primary transcription)
GROQ_API_KEY=your_groq_api_key_here

# Notion Integration Configuration
NOTION_API_KEY=your_notion_integration_secret_here
NOTION_DATABASE_ID=your_notion_database_id_here

# Voice Notes Monitoring Configuration
VOICE_NOTES_FOLDER=your_voice_notes_folder_path_here

# Processing Schedule Configuration
# Uncomment one of these schedule profiles:

# HIGH MEETING DAYS (every 5 minutes during work hours)
# SCHEDULE_PROFILE=high_frequency
# SCHEDULE_INTERVAL=5
# SCHEDULE_START_TIME=08:00
# SCHEDULE_END_TIME=17:00
# SCHEDULE_DAYS=MON,TUE,WED,THU,FRI

# NORMAL MEETING DAYS (every 15 minutes during work hours)
SCHEDULE_PROFILE=normal_frequency
SCHEDULE_INTERVAL=15
SCHEDULE_START_TIME=08:00
SCHEDULE_END_TIME=17:00
SCHEDULE_DAYS=MON,TUE,WED,THU,FRI

# LOW MEETING DAYS (every 30 minutes during work hours)
# SCHEDULE_PROFILE=low_frequency
# SCHEDULE_INTERVAL=30
# SCHEDULE_START_TIME=08:00
# SCHEDULE_END_TIME=17:00
# SCHEDULE_DAYS=MON,TUE,WED,THU,FRI

# MANUAL ONLY (disable automatic processing)
# SCHEDULE_PROFILE=manual_only
# SCHEDULE_INTERVAL=0

# EXTENDED HOURS (for long meeting days)
# SCHEDULE_PROFILE=extended_hours
# SCHEDULE_INTERVAL=10
# SCHEDULE_START_TIME=07:00
# SCHEDULE_END_TIME=19:00
# SCHEDULE_DAYS=MON,TUE,WED,THU,FRI